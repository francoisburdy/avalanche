<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.miage.avalanche" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>Avalanche</name>
    <description>
        Application de gestion du personnel d'intervention et d'organisation des secours d'avalanche
    </description>
    <author email="francois.burdy@gmail.com" href="http://francoisburdy.com/">
        Fran√ßois BURDY
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>

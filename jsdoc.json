{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": [
            "www/",
            "www/js",
            "www/modules/dashboard",
            "www/modules/historique",
            "www/modules/home",
            "www/modules/intervenants",
            "www/modules/journal",
            "www/modules/filters",
            "www/modules/menu",
            "www/modules/parametres",
            "www/modules/services",
            "www/modules/victimes"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "node_modules/jsdoc/plugins/markdown",
        "node_modules/angular-jsdoc/common/plugins/ngdoc"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": true
        }
    }
}
